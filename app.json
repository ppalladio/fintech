{
    "expo": {
        "name": "fintech2",
        "slug": "fintech2",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/images/icon.png",
        "scheme": "myapp",
        "userInterfaceStyle": "automatic",
        "splash": {
            "image": "./assets/images/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "supportsTablet": true,
            "bundleIdentifier": "com.yuxuanize.fintech"
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/images/adaptive-icon.png",
                "backgroundColor": "#ffffff"
            },
            "package": "com.yuxuanize.fintech"
        },
        "web": {
            "bundler": "metro",
            "output": "server",
            "favicon": "./assets/images/favicon.png"
        },
        "plugins": [
            [
                "expo-router",
                {
                    "origin": "https://yuxuanize.com"
                }
            ],
            "expo-secure-store"
        ],
        "experiments": {
            "typedRoutes": true
        },
        "extra": {
            "router": {
                "origin": false
            },
            "eas": {
                "projectId": "f4439fdc-391f-49df-922b-bb225fd98734"
            }
        }
    }
}
